{
  "name": "condun",
  "private": true,
  "author": "@bennettdams",
  "scripts": {
    "/////////////// Meta": "",
    "postinstall": "npm run p:generate",
    "/////////////// RUN": "",
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "devprod": "npm run build && npm run start",
    "/////////////// TEST": "",
    "lint": "next lint",
    "lint2": "eslint . --ext ts --ext tsx --ext js",
    "type-check": "tsc --pretty --noEmit",
    "tc": "npm run type-check",
    "format": "prettier --write ./src/",
    "/////////////// DB": "",
    "p:dev": "npx prisma migrate dev --schema ./src/prisma/schema.prisma",
    "p:dev-create": "npx prisma migrate dev --create-only --schema ./src/prisma/schema.prisma",
    "p:create": "npx prisma migrate dev --create-only --schema ./src/prisma/schema.prisma",
    "p:studio": "npx prisma studio --schema ./src/prisma/schema.prisma",
    "p:reset": "npx prisma migrate reset --schema ./src/prisma/schema.prisma",
    "p:reset-no-seed": "npx prisma migrate reset --skip-seed --schema ./src/prisma/schema.prisma",
    "p:generate": "npx prisma generate --schema ./src/prisma/schema.prisma",
    "p:format": "npx prisma format --schema ./src/prisma/schema.prisma",
    "p:seed": "npx prisma db seed",
    "p:validate": "npx prisma validate --schema ./src/prisma/schema.prisma",
    "/////////////// SETUP": "",
    "check-updates": "npx npm-check-updates -u",
    "cu": "npm run check-updates",
    "check": "npm run tc && npm run lint",
    "clean-install": "rm -rf .next && rm -rf node_modules && npm i"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@hookform/resolvers": "^2.9.7",
    "@prisma/client": "^4.3.1",
    "@supabase/supabase-js": "^1.35.6",
    "@trpc/client": "^9.27.1",
    "@trpc/next": "^9.27.1",
    "@trpc/react": "^9.27.1",
    "@trpc/server": "^9.27.1",
    "formidable": "^2.0.1",
    "next": "^12.2.5",
    "plaiceholder": "^2.4.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.34.2",
    "react-query": "^3.39.2",
    "sharp": "^0.30.7",
    "superjson": "^1.9.1",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.2",
    "@types/formidable": "^2.0.5",
    "@types/node": "^17.0.38",
    "@types/react": "17.0.43",
    "@types/sharp": "^0.30.5",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "autoprefixer": "^10.4.8",
    "eslint": "8.22.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prisma": "^4.3.1",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "4.6.4"
  },
  "//devDependencies": [
    "autoprefixer & postcss for Tailwind",
    "eslint-config-next contains eslint-plugin-react, eslint-plugin-react-hooks and eslint-plugin-next. https://nextjs.org/docs/basic-features/eslint#eslint-config",
    "ts-node is only used for Prisma's seeding functionality"
  ],
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/prisma/seed.ts"
  }
}
