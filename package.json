{
  "name": "condun",
  "private": true,
  "author": "@bennettdams",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "devprod": "npm run build && npm run start",
    "lint": "next lint",
    "lint2": "eslint . --ext ts --ext tsx --ext js",
    "type-check": "tsc --pretty --noEmit",
    "tc": "npm run type-check",
    "format": "prettier --write ./src/",
    "check-updates": "npx npm-check-updates -u",
    "cu": "npm run check-updates",
    "check": "npm run tc && npm run lint",
    "p:dev": "npx prisma migrate dev --schema ./src/prisma/schema.prisma",
    "p:create": "npx prisma migrate dev --create-only --schema ./src/prisma/schema.prisma",
    "p:studio": "npx prisma studio --schema ./src/prisma/schema.prisma",
    "p:reset": "npx prisma migrate reset --schema ./src/prisma/schema.prisma",
    "p:generate": "npx prisma generate --schema ./src/prisma/schema.prisma",
    "p:format": "npx prisma format --schema ./src/prisma/schema.prisma",
    "p:seed": "npx prisma db seed"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "@prisma/client": "^3.12.0",
    "@supabase/supabase-js": "^1.33.3",
    "formidable": "^2.0.1",
    "next": "^12.1.4",
    "plaiceholder": "^2.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.34.19",
    "sharp": "^0.30.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.0",
    "@types/formidable": "^2.0.4",
    "@types/node": "^17.0.24",
    "@types/react": "17.0.43",
    "@types/sharp": "^0.30.2",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "autoprefixer": "^10.4.4",
    "eslint": "8.13.0",
    "eslint-config-next": "^12.1.4",
    "eslint-config-prettier": "^8.5.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "prisma": "^3.12.0",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.7.0",
    "typescript": "4.6.3"
  },
  "//devDependencies": [
    "autoprefixer & postcss for Tailwind",
    "eslint-config-next contains eslint-plugin-react, eslint-plugin-react-hooks and eslint-plugin-next. https://nextjs.org/docs/basic-features/eslint#eslint-config",
    "ts-node is only used for Prisma's seeding functionality"
  ],
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/prisma/seed.ts"
  }
}
